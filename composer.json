{
    "name": "synolia/sylius-scheduler-command-plugin",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin", "scheduler", "command"],
    "description": "Scheduler Command Plugin.",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "dragonmantank/cron-expression": "^3.0",
        "sylius/sylius": "^1.7",
        "symfony/lock": "^3.4|^4.3|^5.1",
        "symfony/service-contracts": "^1.1|^2.0",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/process": "^4.2|^5.1"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "behat/mink-selenium2-driver": "^1.4",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "friendsoftwig/twigcs": "^4.1",
        "j13k/yaml-lint": "^1.1",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpmd/phpmd": "^2.9",
        "phpro/grumphp": "^1.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.1",
        "phpstan/phpstan-doctrine": "^0.12.1",
        "phpstan/phpstan-strict-rules": "^0.12.1",
        "phpstan/phpstan-webmozart-assert": "^0.12.1",
        "phpunit/phpunit": "^9.0",
        "sebastian/phpcpd": "^6.0.2",
        "seld/jsonlint": "^1.7",
        "sensiolabs/security-checker": "^5.0",
        "sivaschenko/utility-cron": "^1.0",
        "sylius-labs/coding-standard": "^4.0",
        "symfony/browser-kit": "^3.4|^4.3|^5.1",
        "symfony/debug-bundle": "^3.4|^4.3|^5.1",
        "symfony/dotenv": "^4.3|^5.1",
        "symfony/intl": "^3.4|^4.3|^5.1",
        "symfony/web-profiler-bundle": "^3.4|^4.3|^5.1",
        "symfony/web-server-bundle": "^3.4|^4.3|^5.1"
    },
    "suggest": {
        "sivaschenko/utility-cron": "Allow showing humanized cron expression."
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusSchedulerCommandPlugin\\": "src/",
            "Tests\\Synolia\\SyliusSchedulerCommandPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "config": {
        "sort-packages": true
    }
}
